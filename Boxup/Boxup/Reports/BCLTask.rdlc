      <Query>
        <DataSourceName>BoxConnectionString</DataSourceName>
        <CommandText>Select BoxItem.ItemName,BoxItem.ItemModel,
               Item.ItemName as Model,
               IIF(IsNULL(BoxItem.ItemModel),null,Right(BoxItem.ItemModel,Len(BoxItem.ItemModel)-Instr(5,BoxItem.ItemModel,'A'))) as Length,
               BoxItem.ItemNum,BoxItem.BoxNo,BoxItem.BoxDate
        From BoxItem Inner Join Item
        ON Instr(BoxItem.ItemModel,Item.ItemName)&gt;0
        Where Item.c=2 And BoxItem.BoxDate=?
order By BoxItem.ItemName,BoxItem.ItemModel,BoxItem.ItemNum,BoxITem.BoxNo</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
